{
    "agent": {
      "metrics_collection_interval": 60,
      "run_as_user": "root"
    },
    "logs": {
      "logs_collected": {
        "files": {
          "collect_list": [
            {
              "file_path": "/var/lib/docker/containers/*/*.log",
              "log_group_name": "/ec2/mcp/docker",
              "log_stream_name": "{instance_id}/mcp",
              "timezone": "UTC"
            }
          ]
        }
      }
    }
  }