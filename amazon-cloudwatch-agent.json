{
    "agent": {
      "metrics_collection_interval": 60,
      "run_as_user": "root"
    },
    "logs": {
      "logs_collected": {
        "files": {
          "collect_list": [
            {
              "file_path": "/var/lib/docker/containers/*/*-json.log",
              "log_group_name": "/docker/containers",
              "log_stream_name": "{container_name}",
              "timezone": "UTC",
              "timestamp_format": "%Y-%m-%dT%H:%M:%S.%f"
            }
          ]
        }
      }
    }
  }